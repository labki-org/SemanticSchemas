			"localBasePath": "",
			"remoteExtPath": "SemanticSchemas",
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.semanticschemas.hierarchy.formpreview": {
			"scripts": [
				"resources/ext.semanticschemas.hierarchy.formpreview.js"
			],
			"styles": [
				"resources/ext.semanticschemas.hierarchy.formpreview.css"
			],
			"dependencies": [
				"mediawiki.api",
				"jquery"
			],
			"localBasePath": "",
			"remoteExtPath": "SemanticSchemas",
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.semanticschemas.install": {
			"scripts": [
				"resources/ext.semanticschemas.install.js"
			],
			"localBasePath": "",
			"remoteExtPath": "SemanticSchemas",
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"APIModules": {
		"semanticschemas-hierarchy": "MediaWiki\\Extension\\SemanticSchemas\\Api\\ApiSemanticSchemasHierarchy",
		"semanticschemas-install": "MediaWiki\\Extension\\SemanticSchemas\\Api\\ApiSemanticSchemasInstall",
		"semanticschemas-multicategory": "MediaWiki\\Extension\\SemanticSchemas\\Api\\ApiSemanticSchemasMultiCategory"
	},
	"SpecialPages": {
		"SemanticSchemas": "MediaWiki\\Extension\\SemanticSchemas\\Special\\SpecialSemanticSchemas",
		"CreateSemanticPage": "MediaWiki\\Extension\\SemanticSchemas\\Special\\SpecialCreateSemanticPage"
	},
	"LogTypes": [
		"semanticschemas"
	],
	"LogNames": {
		"semanticschemas": "semanticschemas-log-name"
	},
	"LogHeaders": {
		"semanticschemas": "semanticschemas-log-header"
	},
	"LogActionsHandlers": {
		"semanticschemas/generate": "LogFormatter"
	},
	"MaintenanceScripts": {
		"SemanticSchemas:InstallConfig": "maintenance/installConfig.php"
	},
	"config": {
		"SemanticSchemasRequireApiAuth": {
			"value": false,
			"description": "If true, require 'read' permission to access the hierarchy API"
		},
		"SemanticSchemasRateLimitPerHour": {
			"value": 20,
			"description": "Maximum number of generate operations allowed per hour per user"
		}
	},
	"manifest_version": 2
}