{
	"name": "StructureSync",
	"version": "0.1.0",
	"author": [
		"Your Name"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:StructureSync",
	"descriptionmsg": "structuresync-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "semantic",
	"requires": {
		"MediaWiki": ">= 1.39.0",
		"extensions": {
			"SemanticMediaWiki": "*",
			"PageForms": "*"
		}
	},
	"load_composer_autoloader": true,
	"MessagesDirs": {
		"StructureSync": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"StructureSyncAlias": "StructureSync.alias.php"
	},
	"Hooks": {
		"ParserFirstCallInit": "MediaWiki\\Extension\\StructureSync\\Parser\\DisplayParserFunctions::onParserFirstCallInit"
	},
	"AutoloadClasses": {
		"MediaWiki\\Extension\\StructureSync\\Parser\\DisplayParserFunctions": "src/Parser/DisplayParserFunctions.php",
		"MediaWiki\\Extension\\StructureSync\\Display\\DisplayRenderer": "src/Display/DisplayRenderer.php",
		"MediaWiki\\Extension\\StructureSync\\Display\\DisplaySpecBuilder": "src/Display/DisplaySpecBuilder.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\StructureSync\\": "src/"
	},
	"SpecialPages": {
		"StructureSync": "MediaWiki\\Extension\\StructureSync\\Special\\SpecialStructureSync"
	},
	"LogTypes": [
		"structuresync"
	],
	"LogNames": {
		"structuresync": "structuresync-log-name"
	},
	"LogHeaders": {
		"structuresync": "structuresync-log-header"
	},
	"LogActionsHandlers": {
		"structuresync/import": "LogFormatter",
		"structuresync/export": "LogFormatter",
		"structuresync/generate": "LogFormatter"
	},
	"manifest_version": 2
}

