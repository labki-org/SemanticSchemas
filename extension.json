{
	"name": "StructureSync",
	"version": "0.1.0",
	"author": [
		"Your Name"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:StructureSync",
	"descriptionmsg": "structuresync-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "semantic",
	"requires": {
		"MediaWiki": ">= 1.39.0",
		"extensions": {
			"SemanticMediaWiki": "*",
			"PageForms": "*"
		}
	},
	"load_composer_autoloader": false,
	"MessagesDirs": {
		"StructureSync": [
			"i18n"
		]
