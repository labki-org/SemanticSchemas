{
	"name": "SemanticSchemas",
	"version": "0.1.0",
	"author": [
		"Daniel Aharoni"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:SemanticSchemas",
	"descriptionmsg": "semanticschemas-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "semantic",
	"requires": {
		"MediaWiki": ">= 1.39.0",
		"extensions": {
			"SemanticMediaWiki": "*",
			"PageForms": "*"
		}
	},
	"load_composer_autoloader": false,
	"MessagesDirs": {
		"SemanticSchemas": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"SemanticSchemasAlias": "SemanticSchemas.alias.php"
	},
	"namespaces": [
		{
			"id": 3300,
			"constant": "NS_SUBOBJECT",
			"name": "Subobject",
			"content": true
		},
		{
			"id": 3301,
			"constant": "NS_SUBOBJECT_TALK",
			"name": "Subobject_talk"
		}
	],
	"namespaceAliases": {
		"Subobject": "NS_SUBOBJECT",
		"Subobject_talk": "NS_SUBOBJECT_TALK"
	},
	"contentNamespaces": [
		"NS_SUBOBJECT"
	],
	"Hooks": {
		"SetupAfterCache": "MediaWiki\\Extension\\SemanticSchemas\\Hooks\\SemanticSchemasSetupHooks::onSetupAfterCache",
		"LoadExtensionSchemaUpdates": "MediaWiki\\Extension\\SemanticSchemas\\Hooks\\SemanticSchemasSetupHooks::onLoadExtensionSchemaUpdates",
		"ParserFirstCallInit": "MediaWiki\\Extension\\SemanticSchemas\\Parser\\DisplayParserFunctions::onParserFirstCallInit",
		"SkinTemplateNavigation::Universal": "MediaWiki\\Extension\\SemanticSchemas\\Hooks\\CategoryPageHooks::onSkinTemplateNavigation"
	},
	"AutoloadClasses": {
		"MediaWiki\\Extension\\SemanticSchemas\\Hooks\\SemanticSchemasSetupHooks": "src/Hooks/SemanticSchemasSetupHooks.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\SemanticSchemas\\": "src/"
	},
	"ResourceModules": {
