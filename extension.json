		},
		{
			"id": 3301,
			"constant": "NS_SUBOBJECT_TALK",
			"name": "Subobject_talk"
		}
	],
	"namespaceAliases": {
		"Subobject": "NS_SUBOBJECT",
		"Subobject_talk": "NS_SUBOBJECT_TALK"
	},
	"contentNamespaces": [
		"NS_SUBOBJECT"
	],
	"Hooks": {
		"SetupAfterCache": "MediaWiki\\Extension\\StructureSync\\Hooks\\StructureSyncSetupHooks::onSetupAfterCache",
		"LoadExtensionSchemaUpdates": "MediaWiki\\Extension\\StructureSync\\Hooks\\StructureSyncSetupHooks::onLoadExtensionSchemaUpdates",
		"ParserFirstCallInit": "MediaWiki\\Extension\\StructureSync\\Parser\\DisplayParserFunctions::onParserFirstCallInit"
	},
	"AutoloadClasses": {
		"MediaWiki\\Extension\\StructureSync\\Hooks\\StructureSyncSetupHooks": "src/Hooks/StructureSyncSetupHooks.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\StructureSync\\": "src/"
	},
	"ResourceModules": {
		"ext.structuresync.styles": {
			"styles": [
				"resources/ext.structuresync.styles.css"
			],
			"localBasePath": "",
			"remoteExtPath": "StructureSync",
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.structuresync.hierarchy": {
			"scripts": [
				"resources/ext.structuresync.hierarchy.js"
			],
			"styles": [
				"resources/ext.structuresync.hierarchy.css"
			],
			"dependencies": [
				"mediawiki.api",
				"mediawiki.util",
				"jquery"
			],
			"messages": [
				"structuresync-hierarchy-no-data",
				"structuresync-hierarchy-no-properties",
				"structuresync-hierarchy-loading",
				"structuresync-hierarchy-error",
				"structuresync-hierarchy-no-category",
				"structuresync-hierarchy-tree-title",
				"structuresync-hierarchy-props-title",
				"structuresync-hierarchy-subobjects-title",
				"structuresync-hierarchy-source-category",
				"structuresync-hierarchy-properties",
				"structuresync-hierarchy-subobject-name",
				"structuresync-hierarchy-required-state",
				"structuresync-hierarchy-unknown-category",
				"structuresync-hierarchy-unnamed-property",
				"structuresync-hierarchy-required",
				"structuresync-hierarchy-optional"
			],
			"localBasePath": "",
			"remoteExtPath": "StructureSync",
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.structuresync.hierarchy.formpreview": {
			"scripts": [
				"resources/ext.structuresync.hierarchy.formpreview.js"
			],
			"styles": [
				"resources/ext.structuresync.hierarchy.formpreview.css"
			],
			"dependencies": [
				"mediawiki.api",
				"jquery"
			],
			"localBasePath": "",
			"remoteExtPath": "StructureSync",
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"APIModules": {
		"structuresync-hierarchy": "MediaWiki\\Extension\\StructureSync\\Api\\ApiStructureSyncHierarchy"
	},
	"SpecialPages": {
		"StructureSync": "MediaWiki\\Extension\\StructureSync\\Special\\SpecialStructureSync"
	},
	"LogTypes": [
		"structuresync"
	],
	"LogNames": {
		"structuresync": "structuresync-log-name"
	},
	"LogHeaders": {
		"structuresync": "structuresync-log-header"
	},
	"LogActionsHandlers": {
		"structuresync/generate": "LogFormatter"
	},
